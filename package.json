{
  "name": "PROJECT-SLUG",
  "description": "Front Base for React Admin",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "%PROJECT-REPO%"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "dev": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "eject": "react-scripts-ts eject",
    "webpack-analyze": "node ./scripts/webpack-analyze.js",
    "update-base": "git pull seed master && git merge seed/master --no-commit --no-ff && yarn rimraf ./init.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "concurrently -r \"yarn tslint -p tsconfig.json\" \"yarn tsc --noEmit\"",
      "post-merge": "yarn"
    }
  },
  "dependencies": {
    "@material-ui/core": "3.0.2",
    "@react-form-fields/material-ui": "1.7.3",
    "axios": "0.18.0",
    "lodash": "4.17.10",
    "luxon": "1.3.3",
    "mdi-react": "4.3.0",
    "query-string": "6.1.0",
    "raven-js": "3.27.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-image-cropper": "1.3.0",
    "react-router-dom": "4.3.1",
    "react-scripts-ts": "2.17.0",
    "react-swipeable-views": "0.12.17",
    "recharts": "1.1.0",
    "rxjs": "6.3.2",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@types/jest": "23.3.1",
    "@types/lodash": "4.14.116",
    "@types/luxon": "1.2.2",
    "@types/node": "10.9.4",
    "@types/query-string": "6.1.0",
    "@types/react": "16.4.13",
    "@types/react-dom": "16.0.7",
    "@types/react-router-dom": "4.3.0",
    "@types/react-swipeable-views": "0.12.2",
    "@types/recharts": "1.0.28",
    "concurrently": "4.0.1",
    "husky": "1.0.0-rc.9",
    "inquirer": "6.2.0",
    "ora": "3.0.0",
    "rimraf": "2.6.2",
    "tslint": "5.11.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "3.6.0",
    "typescript": "3.0.3",
    "webpack-bundle-analyzer": "2.13.1"
  }
}