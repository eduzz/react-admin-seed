    "@types/react-dom": "16.0.7",
    "@types/react-router-dom": "4.3.0",
    "@types/react-swipeable-views": "0.12.2",
    "@types/recharts": "1.0.27",
    "concurrently": "3.6.1",
    "husky": "1.0.0-rc.9",
    "tslint": "5.11.0",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.0.1",
    "webpack-bundle-analyzer": "2.13.1"
  }
