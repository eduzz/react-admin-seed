{
  "name": "nutror-v3-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "lint": "tslint -p . && tsc --noEmit",
    "precommit": "yarn lint",
    "prepush": "yarn lint"
  },
  "dependencies": {
    "@types/react-redux": "^5.0.15",
    "@types/react-slick": "^0.15.1",
    "@types/redux": "^3.6.0",
    "@types/uuid": "^3.4.3",
    "axios": "0.18.0",
    "material-ui": "^1.0.0-beta.37",
    "material-ui-icons": "^1.0.0-beta.36",
    "materialize-css": "^0.100.2",
    "react": "16.2.0",
    "react-color": "^2.13.8",
    "react-dom": "16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.13.0",
    "react-slick": "^0.22.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "slick-carousel": "^1.8.1",
    "tslib": "1.9.0",
    "validatorjs": "3.14.2"
  },
  "devDependencies": {
    "@types/jest": "22.1.4",
    "@types/node": "9.4.6",
    "@types/react": "16.0.40",
    "@types/react-dom": "16.0.4",
    "@types/react-helmet": "^5.0.5",
    "@types/react-router-dom": "^4.2.4",
    "@types/sinon": "^4.3.0",
    "@types/validatorjs": "3.7.1",
    "eslint": "4.18.1",
    "eslint-plugin-react": "7.7.0",
    "husky": "^0.14.3",
    "node-sass-chokidar": "^0.0.3",
    "sinon": "^4.4.2",
    "source-map-explorer": "^1.5.0",
    "tslint": "5.9.1",
    "tslint-eslint-rules": "5.1.0",
    "typescript": "2.7.2"
  }
}
