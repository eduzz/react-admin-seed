{
  "name": "waproject-base-front",
  "description": "Front Base for WaProject",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "waproject-repository"
  },
  "scripts": {
    "dev": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "eject": "react-scripts-ts eject",
    "docker:release": "yarn docker:build && yarn docker:push",
    "docker:build": "docker build -t waproject/front-base -f docker/prod/Dockerfile .",
    "docker:push": "docker push waproject/front-base",
    "webpack-analyze": "node ./scripts/webpack-analyze.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "concurrently -r \"yarn tslint -p tsconfig.json\" \"yarn tsc --noEmit\"",
      "post-merge": "yarn"
    }
  },
  "dependencies": {
    "@material-ui/core": "1.4.0",
    "@react-form-fields/material-ui": "1.5.3",
    "axios": "0.18.0",
    "lodash": "4.17.10",
    "luxon": "1.3.1",
    "mdi-react": "4.0.0",
    "query-string": "6.1.0",
    "raven-js": "3.26.3",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-image-cropper": "1.3.0",
    "react-router-dom": "4.3.1",
    "react-scripts-ts": "2.16.0",
    "react-swipeable-views": "0.12.15",
    "recharts": "1.0.1",
    "rxjs": "6.2.2",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@types/jest": "23.3.0",
    "@types/lodash": "4.14.112",
    "@types/luxon": "1.2.1",
    "@types/node": "10.5.2",
    "@types/query-string": "6.1.0",
    "@types/react": "16.4.6",
    "@types/react-dom": "16.0.6",
    "@types/react-router-dom": "4.2.7",
    "@types/react-swipeable-views": "0.12.1",
    "@types/recharts": "1.0.24",
    "concurrently": "3.6.0",
    "husky": "1.0.0-rc.9",
    "inquirer": "6.0.0",
    "ora": "3.0.0",
    "rimraf": "2.6.2",
    "tslint": "5.11.0",
    "tslint-eslint-rules": "5.3.1",
    "typescript": "2.9.2",
    "webpack-bundle-analyzer": "2.13.1"
  }
}