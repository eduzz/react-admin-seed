    "raven-for-redux": "1.3.1",
    "raven-js": "3.25.1",
    "raven.js": "0.1.0",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-scripts-ts": "2.15.1",
    "redux": "4.0.0",
    "redux-thunk": "2.2.0",
