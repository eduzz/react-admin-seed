    "luxon": "1.3.3",
    "mdi-react": "4.1.0",
    "query-string": "6.1.0",
    "raven-js": "3.26.4",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-image-cropper": "1.3.0",
    "react-router-dom": "4.3.1",
    "react-scripts-ts": "2.17.0",
    "react-swipeable-views": "0.12.16",
    "recharts": "1.1.0",
    "rxjs": "6.2.2",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@types/jest": "23.3.1",
    "@types/lodash": "4.14.116",
    "@types/luxon": "1.2.2",
    "@types/node": "10.7.1",
